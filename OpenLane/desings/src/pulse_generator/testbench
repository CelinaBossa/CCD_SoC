    .scope S_0x5f034ff92d20;
T_1 ;
    %wait E_0x5f034ff94100;
    %load/vec4 v0x5f034ffab840_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 11;
    %assign/vec4 v0x5f034ffab760_0, 0;
    %jmp T_1.1;
T_1.0 ;
