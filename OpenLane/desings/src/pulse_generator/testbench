E_0x5f034ff94100 .event posedge, v0x5f034ffab5e0_0;
L_0x5f034ffabe80 .concat [ 11 21 0 0], v0x5f034ffab760_0, L_0x74d27a89f018;
L_0x5f034ffbc010 .concat [ 4 28 0 0], v0x5f034ffaba60_0, L_0x74d27a89f060;
L_0x5f034ffbc1b0 .arith/sub 32, L_0x5f034ffbc010, L_0x74d27a89f0a8;
L_0x5f034ffbc320 .cmp/eq 32, L_0x5f034ffabe80, L_0x5f034ffbc1b0;
L_0x5f034ffbc4e0 .functor MUXZ 1, L_0x74d27a89f138, L_0x74d27a89f0f0, L_0x5f034ffbc320, C4<>;
    .scope S_0x5f034ff92d20;
