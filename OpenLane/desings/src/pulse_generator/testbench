    .scope S_0x5f034ff92d20;
T_2 ;
    %wait E_0x5f034ff94100;
    %load/vec4 v0x5f034ffab760_0;
    %pad/u 32;
    %load/vec4 v0x5f034ff7f9f0_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %cmp/e;
    %flag_mov 8, 4;
    %jmp/0 T_2.0, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_2.1, 8;
T_2.0 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_2.1, 8;
 ; End of false expr.
    %blend;
T_2.1;
    %assign/vec4 v0x5f034ffab900_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5f034ff92b90;
T_3 ;
