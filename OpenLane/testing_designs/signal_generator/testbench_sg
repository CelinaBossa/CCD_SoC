:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x611feb46d900 .scope module, "test_signal_generator" "test_signal_generator" 2 3;
 .timescale -9 -12;
v0x611feb4857f0_0 .var "clk", 0 0;
v0x611feb4858b0_0 .net "phi_l1", 0 0, v0x611feb485220_0;  1 drivers
v0x611feb485980_0 .net "phi_l2", 0 0, v0x611feb4852e0_0;  1 drivers
v0x611feb485a80_0 .net "phi_p", 0 0, v0x611feb4853f0_0;  1 drivers
v0x611feb485b50_0 .net "phi_r", 0 0, v0x611feb4854b0_0;  1 drivers
v0x611feb485bf0_0 .var "sel", 3 0;
S_0x611feb46da90 .scope module, "uut" "signal_generator" 2 11, 3 1 0, S_0x611feb46d900;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 4 "sel";
    .port_info 2 /OUTPUT 1 "phi_l1";
    .port_info 3 /OUTPUT 1 "phi_l2";
    .port_info 4 /OUTPUT 1 "phi_p";
    .port_info 5 /OUTPUT 1 "phi_r";
P_0x611feb46dc70 .param/l "MIN_P_CYCLES" 0 3 16, +C4<00000000000000000000010000011000>;
v0x611feb45c660_0 .net "clk", 0 0, v0x611feb4857f0_0;  1 drivers
v0x611feb485050_0 .var "l_counter", 5 0;
v0x611feb485130_0 .var "p_counter", 13 0;
v0x611feb485220_0 .var "phi_l1", 0 0;
v0x611feb4852e0_0 .var "phi_l2", 0 0;
v0x611feb4853f0_0 .var "phi_p", 0 0;
v0x611feb4854b0_0 .var "phi_r", 0 0;
v0x611feb485570_0 .var "r_counter", 5 0;
v0x611feb485650_0 .net "sel", 3 0, v0x611feb485bf0_0;  1 drivers
E_0x611feb46eed0 .event posedge, v0x611feb45c660_0;
    .scope S_0x611feb46da90;
T_0 ;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x611feb485050_0, 0, 6;
    %pushi/vec4 0, 0, 14;
    %store/vec4 v0x611feb485130_0, 0, 14;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x611feb485570_0, 0, 6;
    %end;
    .thread T_0;
    .scope S_0x611feb46da90;
T_1 ;
    %wait E_0x611feb46eed0;
    %load/vec4 v0x611feb485050_0;
    %pad/u 32;
    %cmpi/e 6, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x611feb485220_0;
    %inv;
    %assign/vec4 v0x611feb485220_0, 0;
    %load/vec4 v0x611feb4852e0_0;
    %inv;
    %assign/vec4 v0x611feb4852e0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x611feb485050_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x611feb485050_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x611feb485050_0, 0;
T_1.1 ;
    %load/vec4 v0x611feb485220_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 1048, 0, 32;
    %load/vec4 v0x611feb485650_0;
    %pad/u 32;
    %add;
    %load/vec4 v0x611feb485130_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_1.4, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x611feb4853f0_0, 0;
    %pushi/vec4 0, 0, 14;
    %assign/vec4 v0x611feb485130_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x611feb4853f0_0, 0;
    %load/vec4 v0x611feb485130_0;
    %addi 1, 0, 14;
    %assign/vec4 v0x611feb485130_0, 0;
T_1.5 ;
T_1.2 ;
    %load/vec4 v0x611feb485570_0;
    %pad/u 32;
    %cmpi/u 3, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_1.6, 5;
    %load/vec4 v0x611feb4854b0_0;
    %inv;
    %assign/vec4 v0x611feb4854b0_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x611feb485570_0, 0;
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x611feb485570_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x611feb485570_0, 0;
T_1.7 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x611feb46d900;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "wf_signal_generatir.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x611feb46d900 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x611feb4857f0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x611feb485bf0_0, 0, 4;
    %delay 100000, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x611feb485bf0_0, 0, 4;
    %delay 100000, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x611feb485bf0_0, 0, 4;
    %delay 100000, 0;
    %vpi_call 2 35 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x611feb46d900;
T_3 ;
    %delay 10000, 0;
    %load/vec4 v0x611feb4857f0_0;
    %inv;
    %store/vec4 v0x611feb4857f0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
