#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5649d801ed90 .scope module, "testbench" "testbench" 2 3;
 .timescale 0 0;
v0x5649d806dcf0_0 .var "clk_in", 0 0;
v0x5649d806ddb0_0 .net "clk_out", 0 0, v0x5649d806d8d0_0;  1 drivers
v0x5649d806de80_0 .var "div", 31 0;
v0x5649d806df80_0 .var "reset", 0 0;
S_0x5649d801ef20 .scope module, "uut" "my_divider" 2 10, 3 1 0, S_0x5649d801ed90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk_in";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 32 "div";
    .port_info 3 /OUTPUT 1 "clk_out";
v0x5649d8020ba0_0 .net "clk_in", 0 0, v0x5649d806dcf0_0;  1 drivers
v0x5649d806d8d0_0 .var "clk_out", 0 0;
v0x5649d806d990_0 .var "counter", 31 0;
v0x5649d806da80_0 .net "div", 31 0, v0x5649d806de80_0;  1 drivers
v0x5649d806db60_0 .net "reset", 0 0, v0x5649d806df80_0;  1 drivers
E_0x5649d805b190 .event posedge, v0x5649d806db60_0, v0x5649d8020ba0_0;
    .scope S_0x5649d801ef20;
T_0 ;
    %wait E_0x5649d805b190;
    %load/vec4 v0x5649d806db60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5649d806d990_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5649d806d8d0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5649d806da80_0;
    %subi 1, 0, 32;
    %load/vec4 v0x5649d806d990_0;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5649d806d990_0, 0;
    %load/vec4 v0x5649d806d8d0_0;
    %inv;
    %assign/vec4 v0x5649d806d8d0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5649d806d990_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5649d806d990_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5649d801ed90;
T_1 ;
    %vpi_call 2 20 "$dumpfile", "wf_my_divider.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5649d801ed90 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5649d806dcf0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x5649d806dcf0_0;
    %inv;
    %store/vec4 v0x5649d806dcf0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5649d801ed90;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5649d806df80_0, 0, 1;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x5649d806de80_0, 0, 32;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5649d806df80_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x5649d806de80_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 16, 0, 32;
    %store/vec4 v0x5649d806de80_0, 0, 32;
    %delay 100, 0;
    %pushi/vec4 32, 0, 32;
    %store/vec4 v0x5649d806de80_0, 0, 32;
    %delay 500, 0;
    %vpi_call 2 41 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5649d801ed90;
T_3 ;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
